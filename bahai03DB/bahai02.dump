        )
    VALUES(
        v_person_id,
        p_last_name,
        p_first_name,
        p_is_male,
        p_language,
        p_language_2nd,
        p_date_of_birth,
        p_date_of_death,
        p_bahai_cmty_id,
        p_bahai_id_country,
        p_bahai_id,
        p_date_became_bahai,
        p_is_deprived,
        p_age_category,
        p_occupation,
        p_is_healthcare_provider,
        p_edit_errors_group_id
    );

    RETURN v_person_id;

END;
